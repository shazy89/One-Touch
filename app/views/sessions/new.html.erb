<h1>Login</h1>
 <p class="red"><%= flash[:alert] %></p>
<%= form_for @user, url: sessions_path do |f| %>

  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <%= f.submit "Login" %>
<% end %>
  
 <button class="waves-effect waves-red btn-small black"><%= link_to "Sign Up With Github", "/auth/github" %></button>